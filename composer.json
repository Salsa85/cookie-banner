{
    "name": "salsa85/cookie-banner",
    "description": "A modern, customizable cookie consent banner for Laravel and Statamic applications",
    "type": "statamic-addon",
    "license": "MIT",
    "version": "0.1.0",
    "authors": [
        {
            "name": "R. de la Cour",
            "email": "info@zencule.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Salsa85\\CookieBanner\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Salsa85\\CookieBanner\\Tests\\": "tests"
        }
    },
    "extra": {
        "statamic": {
            "name": "Cookie Banner",
            "description": "A modern cookie consent banner"
        },
        "laravel": {
            "providers": [
                "Salsa85\\CookieBanner\\CookieBannerServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "statamic/cms": "^4.0|^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0",
        "laravel/pint": "^1.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 